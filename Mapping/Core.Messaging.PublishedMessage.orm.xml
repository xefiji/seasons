<doctrine-mapping>
    <entity name="Xefiji\Seasons\Messaging\PublishedMessage" table="domain_message">
        <id name="trackerId" type="integer">
            <generator strategy="AUTO"/>
        </id>

        <field name="mostRecentPublishedMessageId" type="bigint"/>
        <field name="exchangeName" type="string"/>
    </entity>
</doctrine-mapping>